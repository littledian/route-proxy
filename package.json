    "mysql2": "^2.1.0"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.4",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
